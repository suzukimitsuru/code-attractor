{
  "name": "vscode-code-attractor",
  "displayName": "code attractor",
  "description": "Visualizing code with attracting force.",
  "version": "0.0.1",
  "preview": true,
	"publisher": "suzukimitsuru",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Visualization"
  ],
  "activationEvents": [],
	"repository": {
		"url": "https://github.com/suzukimitsuru/vscode-code-attractor"
	},
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codeattractor.showEditor",
        "title": "Show Editor",
        "shortTitle": "show",
        "category": "Code Attractor",
        "icon": "media/codeattractor-icon.svg",
        "enablement": "true"
      }
    ],
		"views": {
			"explorer": [
				{
					"id": "codeattractor.views.sidebar",
					"name": "Code Attractor",
          "type": "webview",
          "icon": "media/codeattractor-icon.svg"
				}
			]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.81.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "ts-loader": "^9.4.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1",
    "@vscode/test-electron": "^2.3.2"
  }
}
